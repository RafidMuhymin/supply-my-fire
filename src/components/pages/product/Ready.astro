---
import ReadyShared from "@/components/ReadyShared.astro";

const { title, description, cards } = (
  await import(`@/data/products/${Astro.params.product}/ready.json`)
).default as typeof import("@/data/products/media-wall/ready.json");
---

<ReadyShared
  title={title.main}
  titleHighlight={title.highlight}
  description={description}
  cards={cards}
/>
