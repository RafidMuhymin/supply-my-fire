---
import ReadyShared from "@/components/ReadyShared.astro";

const { title, description, cards } = (
  (await import(
    `@/data/locations/${Astro.params.location}/ready.ts`
  )) as typeof import("@/data/locations/cardiff/ready.ts")
).default;
---

<ReadyShared
  title={title.main}
  titleHighlight={title.highlight}
  description={description}
  cards={cards}
/>
